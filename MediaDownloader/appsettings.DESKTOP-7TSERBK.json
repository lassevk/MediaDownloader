{
    "Logging": {
        "Destinations": {
            "File": {
                "LogLevel": "Verbose",
                "Enabled": true,
                "DirectoryPath": "D:\\Logs\\MediaDownloader",
                "Filename": "media-downloader-{0:yyyy-MM-dd}.log"
            }
        }
    },
    "Media": {
        "Cameras": {
            "Labels": [
                "OsmoPocket"
            ],
            "Target": "${env.DROPBOX}\\Media\\Foto\\Inbox\\{yyyy_MM_dd}\\{filename}",
            "Source": "{ROOT}\\DCIM",
            "Masks": [
                "*.jpg",
                "*.jpeg",
                "*.cr2",
                "*.mp4"
            ],
            "Operation": "Move",
            "Eject": true
        }
    },
    "CameraDownload": {
        "Target": "${env.DROPBOX}\\Media\\Foto\\Inbox\\{yyyy_MM_dd}\\{filename}",
        "Cameras": {
            "OsmoPocket": {
                "Source": "{ROOT}\\DCIM",
                "Operation": "Move",
                "Eject": true
            },
            "H1N_SD": {
                "Source": "{ROOT}\\STEREO",
                "Operation": "Move",
                "Eject": true
            },
            "VP_10": {
                "Source": "{ROOT}\\RECORDER",
                "Operation": "Move",
                "Eject": true
            }
        }
    },
    "Features": {
        "Services": {
            "MediaDownloader": {
                "*": true
            }
        }
    }
}